{
  "name": "obsidize-phrag",
  "version": "1.3.6",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --open",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "copy:prod:build": "node ./scripts/copy-prod-build.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "version:copy": "node ./scripts/copy-version.js",
    "version:patch": "npm version patch",
    "version:publish": "node ./scripts/publish-version.js",
    "version": "run-s version:copy build:prod copy:prod:build",
    "bump": "run-s version:patch version:publish",
    "download:dictionary": "node ./scripts/download-word-dictionary.js",
    "generate:dictionary:module": "node ./scripts/generate-words-module.js",
    "regenerate:dictionary": "run-s download:dictionary generate:dictionary:module"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.1.2",
    "@angular/cdk": "^15.1.0",
    "@angular/common": "^17.1.2",
    "@angular/compiler": "^17.1.2",
    "@angular/core": "^17.1.2",
    "@angular/forms": "^17.1.2",
    "@angular/material": "^15.1.0",
    "@angular/platform-browser": "^17.1.2",
    "@angular/platform-browser-dynamic": "^17.1.2",
    "@angular/router": "^17.1.2",
    "@angular/service-worker": "^17.1.2",
    "pako": "2.1.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.1.2",
    "@angular/cli": "~17.1.2",
    "@angular/compiler-cli": "^17.1.2",
    "@types/jasmine": "~4.3.0",
    "@types/pako": "2.0.0",
    "@types/yargs": "17.0.20",
    "axios": "1.2.2",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm-run-all": "4.1.5",
    "typescript": "~5.3.3",
    "yargs": "17.6.2"
  }
}
