{
  "name": "obsidize-phrag",
  "version": "1.3.2",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "copy:prod:build": "node ./scripts/copy-prod-build.js",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "version:copy": "node ./scripts/copy-version.js",
    "version:patch": "npm version patch",
    "version:publish": "node ./scripts/publish-version.js",
    "version": "run-s version:copy build:prod copy:prod:build",
    "bump": "run-s version:patch version:publish",
    "download:dictionary": "node ./scripts/download-word-dictionary.js",
    "generate:dictionary:module": "node ./scripts/generate-words-module.js",
    "regenerate:dictionary": "run-s download:dictionary generate:dictionary:module"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.0",
    "@angular/cdk": "^15.1.0",
    "@angular/common": "^15.0.0",
    "@angular/compiler": "^15.0.0",
    "@angular/core": "^15.0.0",
    "@angular/forms": "^15.0.0",
    "@angular/material": "^15.1.0",
    "@angular/platform-browser": "^15.0.0",
    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "@angular/service-worker": "^15.0.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.5",
    "@angular/cli": "~15.0.5",
    "@angular/compiler-cli": "^15.0.0",
    "@types/jasmine": "~4.3.0",
    "axios": "1.2.2",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "npm-run-all": "4.1.5",
    "typescript": "~4.8.2"
  }
}
